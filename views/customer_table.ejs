<html lang="en">
<head>
  <meta charset="utf-8">

  <title>The HTML5 Herald</title>
  <meta name="description" content="The HTML5 Herald">
  <meta name="author" content="SitePoint">

  <link rel="stylesheet" href="css/styles.css?v=1.0">

</head>
<script src="https://ajax.googleapis.com/ajax/libs/jquery/2.1.1/jquery.min.js"></script>
<body>


<table border="1">
  <tr>
     <th>customer_id</th>  
     <th>business_name</th>  
     <th>first_name</th>  
     <th>last_name</th>  
     <th>customers_contact_number</th>   
     <th>address_line1</th>  
     <th>address_line2</th>  
     <th>zip_code</th>      
  </tr>
 
<% for(var i=0; i < result.length; i++) { %>
   <tr>
     <td><%= result[i].customer_id %></td>
     <td><%= result[i].business_name %></td>
     <td><%= result[i].first_name %></td>
     <td><%= result[i].last_name %></td>
     <td><%= result[i].customers_contact_number %></td>
     <td><%= result[i].address_line1 %></td>
     <td><%= result[i].address_line2 %></td>
     <td><%= result[i].zip_code %></td>
   </tr>
<% } %>
</table>
